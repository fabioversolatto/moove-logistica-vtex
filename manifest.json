{
  "vendor": "vtexdemoemea",
  "name": "moovelogistica",
  "version": "0.0.0",
  "title": "Moove Logistica for VTEX",
  "description": "Your shipping, tracking and delivery in a linear journey",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "admin": "0.x",
    "store": "0.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "dependencies": {
    "vtexdemoemea.moovelogistica": "0.x",
    "vtex.styleguide": "9.x",
    "vtex.session-client": "1.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "mooveshopifapi.herokuapp.com",
        "path": "/moove_shopify/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.moovelogistica.pt",
        "path": "/moove_api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/{{account}}/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "app.io.vtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "oms.vtexinternal.com.br",
        "path": "/*"
      }
    },
    {
      "name": "AcessaTodosPedidos"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "ADMIN_CRM"
    },
    {
      "name": "LogisticsAdmin"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
